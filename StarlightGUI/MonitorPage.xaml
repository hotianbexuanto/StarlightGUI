<?xml version="1.0" encoding="utf-8" ?>
<Page
    x:Class="StarlightGUI.MonitorPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:essential="using:WinUI3Package"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Background="{StaticResource WindowBackgroundBrush}"
    mc:Ignorable="d">

    <Grid Padding="20,10,20,10">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>

        <!--  固定标题栏区域  -->
        <Grid
            Grid.Row="0"
            Padding="0,0,0,5"
            Background="{StaticResource WindowBackgroundBrush}">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="400" />
                <ColumnDefinition Width="*" />
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="Auto" />
            </Grid.ColumnDefinitions>
            <ScrollView ContentOrientation="Horizontal" HorizontalScrollBarVisibility="Auto">
                <essential:Segmented
                    x:Name="MainSegmented"
                    Grid.Column="0"
                    HorizontalAlignment="Left"
                    SelectedIndex="0"
                    SelectionChanged="MainSegmented_SelectionChanged">
                    <essential:SegmentedItem>
                        <StackPanel Orientation="Horizontal" Spacing="8">
                            <FontIcon
                                FontFamily="Segoe Fluent Icons"
                                FontSize="14"
                                Glyph="&#xf0b2;" />
                            <TextBlock Text="对象" />
                        </StackPanel>
                    </essential:SegmentedItem>
                    <essential:SegmentedItem>
                        <StackPanel Orientation="Horizontal" Spacing="8">
                            <FontIcon
                                FontFamily="Segoe Fluent Icons"
                                FontSize="14"
                                Glyph="&#xebe8;" />
                            <TextBlock Text="调试信息" />
                        </StackPanel>
                    </essential:SegmentedItem>
                    <essential:SegmentedItem x:Name="CallbackSegmentedItem">
                        <StackPanel Orientation="Horizontal" Spacing="8">
                            <FontIcon
                                FontFamily="Segoe Fluent Icons"
                                FontSize="14"
                                Glyph="&#xe77e;" />
                            <TextBlock Text="回调" />
                        </StackPanel>
                    </essential:SegmentedItem>
                    <essential:SegmentedItem x:Name="MiniFilterSegmentedItem">
                        <StackPanel Orientation="Horizontal" Spacing="8">
                            <FontIcon
                                FontFamily="Segoe Fluent Icons"
                                FontSize="14"
                                Glyph="&#xe7e3;" />
                            <TextBlock Text="微过滤驱动" />
                        </StackPanel>
                    </essential:SegmentedItem>
                    <essential:SegmentedItem x:Name="StdFilterSegmentedItem">
                        <StackPanel Orientation="Horizontal" Spacing="8">
                            <FontIcon
                                FontFamily="Segoe Fluent Icons"
                                FontSize="14"
                                Glyph="&#xe7ec;" />
                            <TextBlock Text="标准过滤驱动" />
                        </StackPanel>
                    </essential:SegmentedItem>
                    <essential:SegmentedItem x:Name="SSDTSegmentedItem">
                        <StackPanel Orientation="Horizontal" Spacing="8">
                            <FontIcon
                                FontFamily="Segoe Fluent Icons"
                                FontSize="14"
                                Glyph="&#xf0b9;" />
                            <TextBlock Text="SSDT" />
                        </StackPanel>
                    </essential:SegmentedItem>
                    <essential:SegmentedItem x:Name="SSSDTSegmentedItem">
                        <StackPanel Orientation="Horizontal" Spacing="8">
                            <FontIcon
                                FontFamily="Segoe Fluent Icons"
                                FontSize="14"
                                Glyph="&#xe727;" />
                            <TextBlock Text="Shadow SSDT" />
                        </StackPanel>
                    </essential:SegmentedItem>
                    <essential:SegmentedItem x:Name="IoTimerSegmentedItem">
                        <StackPanel Orientation="Horizontal" Spacing="8">
                            <FontIcon
                                FontFamily="Segoe Fluent Icons"
                                FontSize="14"
                                Glyph="&#xe916;" />
                            <TextBlock Text="I/O 定时器" />
                        </StackPanel>
                    </essential:SegmentedItem>
                    <essential:SegmentedItem x:Name="ExCallbackSegmentedItem">
                        <StackPanel Orientation="Horizontal" Spacing="8">
                            <FontIcon
                                FontFamily="Segoe Fluent Icons"
                                FontSize="14"
                                Glyph="&#xe7f2;" />
                            <TextBlock Text="ExCallback" />
                        </StackPanel>
                    </essential:SegmentedItem>
                    <essential:SegmentedItem x:Name="IDTSegmentedItem">
                        <StackPanel Orientation="Horizontal" Spacing="8">
                            <FontIcon
                                FontFamily="Segoe Fluent Icons"
                                FontSize="14"
                                Glyph="&#xe950;" />
                            <TextBlock Text="中断描述符表" />
                        </StackPanel>
                    </essential:SegmentedItem>
                    <essential:SegmentedItem x:Name="GDTSegmentedItem">
                        <StackPanel Orientation="Horizontal" Spacing="8">
                            <FontIcon
                                FontFamily="Segoe Fluent Icons"
                                FontSize="14"
                                Glyph="&#xe964;" />
                            <TextBlock Text="全局描述符表" />
                        </StackPanel>
                    </essential:SegmentedItem>
                    <essential:SegmentedItem x:Name="PiDDBSegmentedItem">
                        <StackPanel Orientation="Horizontal" Spacing="8">
                            <FontIcon
                                FontFamily="Segoe Fluent Icons"
                                FontSize="14"
                                Glyph="&#xf0e3;" />
                            <TextBlock Text="PiDDB 缓存表" />
                        </StackPanel>
                    </essential:SegmentedItem>
                    <essential:SegmentedItem x:Name="HALDPTSegmentedItem">
                        <StackPanel Orientation="Horizontal" Spacing="8">
                            <FontIcon
                                FontFamily="Segoe Fluent Icons"
                                FontSize="14"
                                Glyph="&#xf590;" />
                            <TextBlock Text="HAL 分发表" />
                        </StackPanel>
                    </essential:SegmentedItem>
                    <essential:SegmentedItem x:Name="HALPDPTSegmentedItem">
                        <StackPanel Orientation="Horizontal" Spacing="8">
                            <FontIcon
                                FontFamily="Segoe Fluent Icons"
                                FontSize="14"
                                Glyph="&#xf58f;" />
                            <TextBlock Text="HAL 私有分发表" />
                        </StackPanel>
                    </essential:SegmentedItem>
                </essential:Segmented>
            </ScrollView>
            <CommandBar
                Grid.Column="2"
                HorizontalAlignment="Right"
                DefaultLabelPosition="Right">
                <AppBarButton
                    x:Name="RefreshButton"
                    Click="RefreshButton_Click"
                    Label="刷新">
                    <AppBarButton.Icon>
                        <FontIcon FontFamily="Segoe Fluent Icons" Glyph="&#xe72c;" />
                    </AppBarButton.Icon>
                </AppBarButton>
            </CommandBar>

            <TextBox
                x:Name="SearchBox"
                Grid.Column="3"
                Width="300"
                Height="32"
                HorizontalAlignment="Right"
                PlaceholderText="搜索..."
                TextChanged="SearchBox_TextChanged" />
        </Grid>

        <ProgressRing
            x:Name="LoadingRing"
            Grid.Row="1"
            HorizontalAlignment="Center"
            VerticalAlignment="Center"
            IsActive="False" />

        <Grid
            x:Name="ObjectGrid"
            Grid.Row="1"
            Visibility="Collapsed">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="*" />
            </Grid.ColumnDefinitions>
            <Border
                Grid.Column="0"
                Background="{ThemeResource ControlFillColorDefaultBrush}"
                BorderBrush="{ThemeResource SystemControlBackgroundBaseLowBrush}"
                BorderThickness="1"
                CornerRadius="5,0,0,5">
                <ScrollView MaxWidth="150" ContentOrientation="Both">
                    <essential:Segmented
                        x:Name="ObjectTreeView"
                        VerticalAlignment="Top"
                        BorderThickness="0"
                        CornerRadius="0"
                        SelectedIndex="0"
                        SelectionChanged="ObjectTreeView_SelectionChanged"
                        Style="{StaticResource ButtonSegmentedStyle}">
                        <essential:Segmented.ItemsPanel>
                            <ItemsPanelTemplate>
                                <StackPanel Orientation="Vertical" />
                            </ItemsPanelTemplate>
                        </essential:Segmented.ItemsPanel>
                    </essential:Segmented>
                </ScrollView>
            </Border>
            <Grid Grid.Column="1">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>
                <Border
                    Grid.Row="0"
                    BorderBrush="{ThemeResource SystemControlBackgroundBaseLowBrush}"
                    BorderThickness="0,1,1,1"
                    CornerRadius="0,5,0,0">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="150" />
                        </Grid.ColumnDefinitions>

                        <Button
                            Grid.Column="0"
                            Height="32"
                            HorizontalAlignment="Stretch"
                            Content="名称"
                            CornerRadius="0" />
                        <Button
                            Grid.Column="1"
                            Height="32"
                            HorizontalAlignment="Stretch"
                            BorderBrush="{ThemeResource SystemControlBackgroundBaseLowBrush}"
                            BorderThickness="1,0,0,0"
                            Content="类型"
                            CornerRadius="0" />
                    </Grid>
                </Border>
                <Border
                    Grid.Row="1"
                    Background="{ThemeResource ControlFillColorDefaultBrush}"
                    BorderBrush="{ThemeResource SystemControlBackgroundBaseLowBrush}"
                    BorderThickness="0,0,1,1"
                    CornerRadius="0,0,5,0">
                    <Grid>
                        <ListView
                            x:Name="ObjectListView"
                            Background="Transparent"
                            Tapped="ObjectListView_Tapped">
                            <ListView.ItemTemplate>
                                <DataTemplate>
                                    <Grid Height="50">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*" />
                                            <ColumnDefinition Width="150" />
                                        </Grid.ColumnDefinitions>

                                        <!--  名称  -->
                                        <StackPanel Grid.Column="0" VerticalAlignment="Center">
                                            <TextBlock
                                                FontSize="14"
                                                Foreground="{ThemeResource SystemControlForegroundBaseHighBrush}"
                                                Text="{Binding Name}" />
                                            <TextBlock
                                                FontSize="12"
                                                Foreground="{ThemeResource SystemControlForegroundBaseMediumBrush}"
                                                Text="{Binding Path}" />
                                        </StackPanel>

                                        <Border
                                            Grid.Column="1"
                                            Margin="12,0,0,0"
                                            HorizontalAlignment="Left"
                                            VerticalAlignment="Stretch"
                                            BorderBrush="{ThemeResource SystemControlBackgroundBaseLowBrush}"
                                            BorderThickness="0,0,1,0" />

                                        <!--  类型  -->
                                        <TextBlock
                                            Grid.Column="1"
                                            Margin="24,0,0,0"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Center"
                                            FontSize="12"
                                            Foreground="{ThemeResource SystemControlForegroundBaseMediumBrush}"
                                            Text="{Binding Type}" />
                                    </Grid>
                                </DataTemplate>
                            </ListView.ItemTemplate>
                        </ListView>
                    </Grid>
                </Border>
            </Grid>
        </Grid>
        <Grid
            x:Name="DbgViewGrid"
            Grid.Row="1"
            Visibility="Collapsed">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto" />
                <RowDefinition Height="*" />
            </Grid.RowDefinitions>
            <Grid Grid.Row="0" Margin="0,0,0,5">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="Auto" />
                </Grid.ColumnDefinitions>
                <Button
                    x:Name="DbgViewButton"
                    Grid.Column="0"
                    Click="DbgViewButton_Click"
                    Content="开启" />
                <CheckBox
                    x:Name="DbgViewGlobalCheckBox"
                    Grid.Column="2"
                    Margin="0,0,-30,0"
                    Click="DbgViewGlobalCheckBox_Click"
                    Content="捕捉全局" />
            </Grid>
            <ScrollView
                Grid.Row="1"
                Background="{ThemeResource ControlFillColorDefaultBrush}"
                BorderBrush="{ThemeResource SystemControlBackgroundBaseLowBrush}"
                BorderThickness="1"
                CornerRadius="5">
                <TextBlock
                    x:Name="DbgViewBox"
                    Grid.Row="1"
                    Margin="5,5,5,5"
                    HorizontalAlignment="Stretch"
                    VerticalAlignment="Stretch"
                    TextWrapping="Wrap" />
            </ScrollView>
        </Grid>
        <Grid
            x:Name="CallbackGrid"
            Grid.Row="1"
            Visibility="Collapsed">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto" />
                <RowDefinition Height="*" />
            </Grid.RowDefinitions>
            <Border
                Grid.Row="0"
                BorderBrush="{ThemeResource SystemControlBackgroundBaseLowBrush}"
                BorderThickness="1"
                CornerRadius="5,5,0,0">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="150" />
                        <ColumnDefinition Width="150" />
                    </Grid.ColumnDefinitions>

                    <Button
                        Grid.Column="0"
                        Height="32"
                        HorizontalAlignment="Stretch"
                        Content="类型/模块"
                        CornerRadius="0" />
                    <Button
                        Grid.Column="1"
                        Height="32"
                        HorizontalAlignment="Stretch"
                        BorderBrush="{ThemeResource SystemControlBackgroundBaseLowBrush}"
                        BorderThickness="1,0,0,0"
                        Content="入口"
                        CornerRadius="0" />
                    <Button
                        Grid.Column="2"
                        Height="32"
                        HorizontalAlignment="Stretch"
                        BorderBrush="{ThemeResource SystemControlBackgroundBaseLowBrush}"
                        BorderThickness="1,0,0,0"
                        Content="句柄"
                        CornerRadius="0" />
                </Grid>
            </Border>
            <Border
                Grid.Row="1"
                Background="{ThemeResource ControlFillColorDefaultBrush}"
                BorderBrush="{ThemeResource SystemControlBackgroundBaseLowBrush}"
                BorderThickness="1,0,1,1"
                CornerRadius="0,0,5,5">
                <Grid>
                    <ListView x:Name="CallbackListView" Background="Transparent">
                        <ListView.ItemTemplate>
                            <DataTemplate>
                                <Grid Height="50">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*" />
                                        <ColumnDefinition Width="150" />
                                        <ColumnDefinition Width="150" />
                                    </Grid.ColumnDefinitions>

                                    <!--  类型/模块  -->
                                    <StackPanel Grid.Column="0" VerticalAlignment="Center">
                                        <TextBlock
                                            FontSize="14"
                                            Foreground="{ThemeResource SystemControlForegroundBaseHighBrush}"
                                            Text="{Binding String2}" />
                                        <TextBlock
                                            FontSize="12"
                                            Foreground="{ThemeResource SystemControlForegroundBaseMediumBrush}"
                                            Text="{Binding String1}" />
                                    </StackPanel>

                                    <Border
                                        Grid.Column="1"
                                        Margin="12,0,0,0"
                                        HorizontalAlignment="Left"
                                        VerticalAlignment="Stretch"
                                        BorderBrush="{ThemeResource SystemControlBackgroundBaseLowBrush}"
                                        BorderThickness="0,0,1,0" />

                                    <!--  入口  -->
                                    <TextBlock
                                        Grid.Column="1"
                                        Margin="24,0,0,0"
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Center"
                                        FontSize="12"
                                        Foreground="{ThemeResource SystemControlForegroundBaseMediumBrush}"
                                        Text="{Binding String3}" />

                                    <Border
                                        Grid.Column="2"
                                        Margin="12,0,0,0"
                                        HorizontalAlignment="Left"
                                        VerticalAlignment="Stretch"
                                        BorderBrush="{ThemeResource SystemControlBackgroundBaseLowBrush}"
                                        BorderThickness="0,0,1,0" />

                                    <!--  句柄  -->
                                    <TextBlock
                                        Grid.Column="2"
                                        Margin="24,0,0,0"
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Center"
                                        FontSize="12"
                                        Foreground="{ThemeResource SystemControlForegroundBaseMediumBrush}"
                                        Text="{Binding String4}" />
                                </Grid>
                            </DataTemplate>
                        </ListView.ItemTemplate>
                    </ListView>
                </Grid>
            </Border>
        </Grid>
        <Grid
            x:Name="MiniFilterGrid"
            Grid.Row="1"
            Visibility="Collapsed">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto" />
                <RowDefinition Height="*" />
            </Grid.RowDefinitions>
            <Border
                Grid.Row="0"
                BorderBrush="{ThemeResource SystemControlBackgroundBaseLowBrush}"
                BorderThickness="1"
                CornerRadius="5,5,0,0">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="150" />
                        <ColumnDefinition Width="150" />
                        <ColumnDefinition Width="150" />
                    </Grid.ColumnDefinitions>

                    <Button
                        Grid.Column="0"
                        Height="32"
                        HorizontalAlignment="Stretch"
                        Content="IRP/模块"
                        CornerRadius="0" />
                    <Button
                        Grid.Column="1"
                        Height="32"
                        HorizontalAlignment="Stretch"
                        BorderBrush="{ThemeResource SystemControlBackgroundBaseLowBrush}"
                        BorderThickness="1,0,0,0"
                        Content="过滤器"
                        CornerRadius="0" />
                    <Button
                        Grid.Column="2"
                        Height="32"
                        HorizontalAlignment="Stretch"
                        BorderBrush="{ThemeResource SystemControlBackgroundBaseLowBrush}"
                        BorderThickness="1,0,0,0"
                        Content="PreFilter"
                        CornerRadius="0" />
                    <Button
                        Grid.Column="3"
                        Height="32"
                        HorizontalAlignment="Stretch"
                        BorderBrush="{ThemeResource SystemControlBackgroundBaseLowBrush}"
                        BorderThickness="1,0,0,0"
                        Content="PostFilter"
                        CornerRadius="0" />
                </Grid>
            </Border>
            <Border
                Grid.Row="1"
                Background="{ThemeResource ControlFillColorDefaultBrush}"
                BorderBrush="{ThemeResource SystemControlBackgroundBaseLowBrush}"
                BorderThickness="1,0,1,1"
                CornerRadius="0,0,5,5">
                <Grid>
                    <ListView x:Name="MiniFilterListView" Background="Transparent">
                        <ListView.ItemTemplate>
                            <DataTemplate>
                                <Grid Height="50">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*" />
                                        <ColumnDefinition Width="150" />
                                        <ColumnDefinition Width="150" />
                                        <ColumnDefinition Width="150" />
                                    </Grid.ColumnDefinitions>

                                    <!--  IRP/模块  -->
                                    <StackPanel Grid.Column="0" VerticalAlignment="Center">
                                        <TextBlock
                                            FontSize="14"
                                            Foreground="{ThemeResource SystemControlForegroundBaseHighBrush}"
                                            Text="{Binding String2}" />
                                        <TextBlock
                                            FontSize="12"
                                            Foreground="{ThemeResource SystemControlForegroundBaseMediumBrush}"
                                            Text="{Binding String1}" />
                                    </StackPanel>

                                    <Border
                                        Grid.Column="1"
                                        Margin="12,0,0,0"
                                        HorizontalAlignment="Left"
                                        VerticalAlignment="Stretch"
                                        BorderBrush="{ThemeResource SystemControlBackgroundBaseLowBrush}"
                                        BorderThickness="0,0,1,0" />

                                    <!--  过滤器  -->
                                    <TextBlock
                                        Grid.Column="1"
                                        Margin="24,0,0,0"
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Center"
                                        FontSize="12"
                                        Foreground="{ThemeResource SystemControlForegroundBaseMediumBrush}"
                                        Text="{Binding String3}" />

                                    <Border
                                        Grid.Column="2"
                                        Margin="12,0,0,0"
                                        HorizontalAlignment="Left"
                                        VerticalAlignment="Stretch"
                                        BorderBrush="{ThemeResource SystemControlBackgroundBaseLowBrush}"
                                        BorderThickness="0,0,1,0" />

                                    <!--  PreFilter  -->
                                    <TextBlock
                                        Grid.Column="2"
                                        Margin="24,0,0,0"
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Center"
                                        FontSize="12"
                                        Foreground="{ThemeResource SystemControlForegroundBaseMediumBrush}"
                                        Text="{Binding String4}" />

                                    <Border
                                        Grid.Column="3"
                                        Margin="12,0,0,0"
                                        HorizontalAlignment="Left"
                                        VerticalAlignment="Stretch"
                                        BorderBrush="{ThemeResource SystemControlBackgroundBaseLowBrush}"
                                        BorderThickness="0,0,1,0" />

                                    <!--  PostFilter  -->
                                    <TextBlock
                                        Grid.Column="3"
                                        Margin="24,0,0,0"
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Center"
                                        FontSize="12"
                                        Foreground="{ThemeResource SystemControlForegroundBaseMediumBrush}"
                                        Text="{Binding String5}" />
                                </Grid>
                            </DataTemplate>
                        </ListView.ItemTemplate>
                    </ListView>
                </Grid>
            </Border>
        </Grid>
        <Grid
            x:Name="StdFilterGrid"
            Grid.Row="1"
            Visibility="Collapsed">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto" />
                <RowDefinition Height="*" />
            </Grid.RowDefinitions>
            <Border
                Grid.Row="0"
                BorderBrush="{ThemeResource SystemControlBackgroundBaseLowBrush}"
                BorderThickness="1"
                CornerRadius="5,5,0,0">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="100" />
                        <ColumnDefinition Width="150" />
                        <ColumnDefinition Width="150" />
                    </Grid.ColumnDefinitions>

                    <Button
                        Grid.Column="0"
                        Height="32"
                        HorizontalAlignment="Stretch"
                        Content="驱动/模块"
                        CornerRadius="0" />
                    <Button
                        Grid.Column="1"
                        Height="32"
                        HorizontalAlignment="Stretch"
                        BorderBrush="{ThemeResource SystemControlBackgroundBaseLowBrush}"
                        BorderThickness="1,0,0,0"
                        Content="类型"
                        CornerRadius="0" />
                    <Button
                        Grid.Column="2"
                        Height="32"
                        HorizontalAlignment="Stretch"
                        BorderBrush="{ThemeResource SystemControlBackgroundBaseLowBrush}"
                        BorderThickness="1,0,0,0"
                        Content="驱动对象"
                        CornerRadius="0" />
                    <Button
                        Grid.Column="3"
                        Height="32"
                        HorizontalAlignment="Stretch"
                        BorderBrush="{ThemeResource SystemControlBackgroundBaseLowBrush}"
                        BorderThickness="1,0,0,0"
                        Content="目标驱动对象"
                        CornerRadius="0" />
                </Grid>
            </Border>
            <Border
                Grid.Row="1"
                Background="{ThemeResource ControlFillColorDefaultBrush}"
                BorderBrush="{ThemeResource SystemControlBackgroundBaseLowBrush}"
                BorderThickness="1,0,1,1"
                CornerRadius="0,0,5,5">
                <Grid>
                    <ListView x:Name="StdFilterListView" Background="Transparent">
                        <ListView.ItemTemplate>
                            <DataTemplate>
                                <Grid Height="50">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*" />
                                        <ColumnDefinition Width="100" />
                                        <ColumnDefinition Width="150" />
                                        <ColumnDefinition Width="150" />
                                    </Grid.ColumnDefinitions>

                                    <!--  驱动/模块  -->
                                    <StackPanel Grid.Column="0" VerticalAlignment="Center">
                                        <TextBlock
                                            FontSize="14"
                                            Foreground="{ThemeResource SystemControlForegroundBaseHighBrush}"
                                            Text="{Binding String2}" />
                                        <TextBlock
                                            FontSize="12"
                                            Foreground="{ThemeResource SystemControlForegroundBaseMediumBrush}"
                                            Text="{Binding String3}" />
                                    </StackPanel>

                                    <Border
                                        Grid.Column="1"
                                        Margin="12,0,0,0"
                                        HorizontalAlignment="Left"
                                        VerticalAlignment="Stretch"
                                        BorderBrush="{ThemeResource SystemControlBackgroundBaseLowBrush}"
                                        BorderThickness="0,0,1,0" />

                                    <!--  类型  -->
                                    <TextBlock
                                        Grid.Column="1"
                                        Margin="24,0,0,0"
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Center"
                                        FontSize="12"
                                        Foreground="{ThemeResource SystemControlForegroundBaseMediumBrush}"
                                        Text="{Binding String1}" />

                                    <Border
                                        Grid.Column="2"
                                        Margin="12,0,0,0"
                                        HorizontalAlignment="Left"
                                        VerticalAlignment="Stretch"
                                        BorderBrush="{ThemeResource SystemControlBackgroundBaseLowBrush}"
                                        BorderThickness="0,0,1,0" />

                                    <!--  驱动对象  -->
                                    <TextBlock
                                        Grid.Column="2"
                                        Margin="24,0,0,0"
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Center"
                                        FontSize="12"
                                        Foreground="{ThemeResource SystemControlForegroundBaseMediumBrush}"
                                        Text="{Binding String4}" />

                                    <Border
                                        Grid.Column="3"
                                        Margin="12,0,0,0"
                                        HorizontalAlignment="Left"
                                        VerticalAlignment="Stretch"
                                        BorderBrush="{ThemeResource SystemControlBackgroundBaseLowBrush}"
                                        BorderThickness="0,0,1,0" />

                                    <!--  目标驱动对象  -->
                                    <TextBlock
                                        Grid.Column="3"
                                        Margin="24,0,0,0"
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Center"
                                        FontSize="12"
                                        Foreground="{ThemeResource SystemControlForegroundBaseMediumBrush}"
                                        Text="{Binding String5}" />
                                </Grid>
                            </DataTemplate>
                        </ListView.ItemTemplate>
                    </ListView>
                </Grid>
            </Border>
        </Grid>
        <Grid
            x:Name="SSDTGrid"
            Grid.Row="1"
            Visibility="Collapsed">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto" />
                <RowDefinition Height="*" />
            </Grid.RowDefinitions>
            <Border
                Grid.Row="0"
                BorderBrush="{ThemeResource SystemControlBackgroundBaseLowBrush}"
                BorderThickness="1"
                CornerRadius="5,5,0,0">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="100" />
                        <ColumnDefinition Width="150" />
                        <ColumnDefinition Width="150" />
                        <ColumnDefinition Width="80" />
                    </Grid.ColumnDefinitions>

                    <Button
                        Grid.Column="0"
                        Height="32"
                        HorizontalAlignment="Stretch"
                        Content="名称/模块"
                        CornerRadius="0" />
                    <Button
                        Grid.Column="1"
                        Height="32"
                        HorizontalAlignment="Stretch"
                        BorderBrush="{ThemeResource SystemControlBackgroundBaseLowBrush}"
                        BorderThickness="1,0,0,0"
                        Content="钩子"
                        CornerRadius="0" />
                    <Button
                        Grid.Column="2"
                        Height="32"
                        HorizontalAlignment="Stretch"
                        BorderBrush="{ThemeResource SystemControlBackgroundBaseLowBrush}"
                        BorderThickness="1,0,0,0"
                        Content="地址"
